# SeedVR RunPod Environment Variables
# Copy this file to .env and fill in your values

# Required: Hugging Face token for model downloads
HF_TOKEN=your_huggingface_token_here

# Gradio Configuration
GRADIO_SHARE=false
GRADIO_PORT=7860
GRADIO_HOST=0.0.0.0

# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Optional: Container name override
CONTAINER_NAME=seedvr-runpod

# Optional: Custom data directory (for docker-compose)
DATA_DIR=./data